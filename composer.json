{
    "name": "extcode/cart-paypal",
    "type": "typo3-cms-extension",
    "description": "Shopping Cart(s) for TYPO3 - PayPal Payment Provider",
    "homepage": "https://cart.extco.de",
    "license": [
        "GPL-2.0+"
    ],
    "keywords": [
        "TYPO3 CMS",
        "Shopping Cart",
        "PayPal",
        "cart"
    ],
    "authors": [
        {
            "name": "Daniel Lorenz",
            "email": "ext.cart@extco.de",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/extcode/cart_paypal/issues"
    },
    "require": {
        "php": ">=5.4.0",
        "typo3/cms-core": "^6.2 || ^7.6 || ^8.7",
        "extcode/cart": ">=1.0.1"
    },
    "require-dev": {
        "nimut/testing-framework": "^1.0",
        "mikey179/vfsStream": "~1.6.0",
        "phpunit/phpunit": "~4.8.0",
        "friendsofphp/php-cs-fixer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Extcode\\CartPaypal\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Extcode\\CartPaypal\\Tests\\": "Tests",
            "TYPO3\\CMS\\Core\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/core/Tests/",
            "TYPO3\\CMS\\Fluid\\Tests\\": ".Build/vendor/typo3/cms/typo3/sysext/fluid/Tests/"
        }
    },
    "replace": {
        "cart_paypal": "self.version"
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": ".Build/Web",
            "Package": {
                "partOfMinimalUsableSystem": true
            }
        }
    }
}